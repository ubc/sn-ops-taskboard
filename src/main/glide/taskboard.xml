<?xml version="1.0" encoding="utf-8" ?>
<j:jelly trim="false" xmlns:j="jelly:core" xmlns:g="glide" xmlns:j2="null" xmlns:g2="null">

	<g2:evaluate id="evaluate2">
		<!-- evaluate2.js will be included here -->
	</g2:evaluate>

	<div xmlns="http://www.w3.org/1999/xhtml">
		<div>ok = $[ok]</div>

		<table>
			<thead>
				<tr>
					<th>Number</th>
					<th>Description</th>
				</tr>
			</thead>
			<tbody>
				<j2:while test="$[todoBoard.next()]">
					<tr>
						<td><a class="linked formlink" href="$[todoBoard.value().link]">$[todoBoard.value().number]</a></td>
						<td>$[todoBoard.value().short_description]</td>
					</tr>
				</j2:while>
			</tbody>
		</table>
	</div>
</j:jelly>
